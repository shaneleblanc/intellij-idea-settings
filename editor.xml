    <option name="LINE_SPACING" value="1.0" />
    <option name="SECONDARY_FONT_FAMILY" value="Fira Code Retina" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
